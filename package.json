{
  "name": "@mish-tv/sortable-list",
  "version": "1.0.0",
  "description": "React component for creating a vertical list UI that can be reordered by drag and drop",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "scripts": {
    "prebuild": "rm -rf ./lib",
    "build": "tsc -b ./tsconfig.build.json ./tsconfig.build-cjs.json",
    "watch": "tsc -b ./tsconfig.build.json ./tsconfig.build-cjs.json --watch",
    "lint": "tsc --noEmit && eslint \"./src/**/*.ts\"",
    "fix:ts": "eslint --fix \"./src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mish-tv/sortable-list.git"
  },
  "author": "Koji Murata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mish-tv/sortable-list/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/mish-tv/sortable-list#readme",
  "devDependencies": {
    "@types/react": "^16.8.25",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-order": "^5.0.0",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
